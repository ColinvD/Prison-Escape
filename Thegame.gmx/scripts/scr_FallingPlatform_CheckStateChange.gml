if (newState != currentState)
{
    switch (newState)
    {
        case FallingPlatformStates.Normal:
        break;
        
        case FallingPlatformStates.PlayerOn:
        break;
        
        case FallingPlatformStates.PlayerOff:
        break;
        
        case FallingPlatformStates.Falling:
        break;
    }
    
    currentState = newState;
}
